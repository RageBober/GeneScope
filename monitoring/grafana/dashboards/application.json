{
  "dashboard": {
    "id": null,
    "title": "GenoScope Application Dashboard",
    "tags": ["genoscope", "application"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
        "type": "graph",
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(fastapi_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "show": true },
          { "format": "short", "show": false }
        ]
      },
      {
        "id": 2,
        "gridPos": { "x": 8, "y": 0, "w": 8, "h": 6 },
        "type": "graph",
        "title": "Response Time (95th percentile)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(fastapi_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "s", "show": true },
          { "format": "short", "show": false }
        ]
      },
      {
        "id": 3,
        "gridPos": { "x": 16, "y": 0, "w": 8, "h": 6 },
        "type": "stat",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "sum(rate(fastapi_requests_total{status=~\"5..\"}[5m])) / sum(rate(fastapi_requests_total[5m]))",
            "refId": "A"
          }
        ],
        "format": "percentunit",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 0.01 },
            { "color": "red", "value": 0.05 }
          ]
        }
      },
      {
        "id": 4,
        "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Pipeline Jobs",
        "targets": [
          {
            "expr": "rate(pipeline_jobs_total[1h])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Active Pipeline Jobs",
        "targets": [
          {
            "expr": "pipeline_jobs_active",
            "legendFormat": "Active Jobs",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "gridPos": { "x": 0, "y": 14, "w": 8, "h": 6 },
        "type": "stat",
        "title": "Total Variants Processed",
        "targets": [
          {
            "expr": "sum(variants_processed_total)",
            "refId": "A"
          }
        ],
        "format": "short"
      },
      {
        "id": 7,
        "gridPos": { "x": 8, "y": 14, "w": 8, "h": 6 },
        "type": "graph",
        "title": "Variant Quality Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(variant_quality_score_bucket[1h]))",
            "legendFormat": "Median Quality",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "gridPos": { "x": 16, "y": 14, "w": 8, "h": 6 },
        "type": "piechart",
        "title": "Subscription Distribution",
        "targets": [
          {
            "expr": "subscription_count",
            "legendFormat": "{{plan}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Database Performance",
        "targets": [
          {
            "expr": "database_query_duration_seconds",
            "legendFormat": "{{query_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
        "type": "graph",
        "title": "Celery Queue Length",
        "targets": [
          {
            "expr": "celery_queue_length",
            "legendFormat": "{{queue_name}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 11,
        "gridPos": { "x": 0, "y": 28, "w": 8, "h": 6 },
        "type": "gauge",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "system_cpu_percent",
            "refId": "A"
          }
        ],
        "format": "percent",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 70 },
            { "color": "red", "value": 90 }
          ]
        }
      },
      {
        "id": 12,
        "gridPos": { "x": 8, "y": 28, "w": 8, "h": 6 },
        "type": "gauge",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "system_memory_percent",
            "refId": "A"
          }
        ],
        "format": "percent",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 80 },
            { "color": "red", "value": 95 }
          ]
        }
      },
      {
        "id": 13,
        "gridPos": { "x": 16, "y": 28, "w": 8, "h": 6 },
        "type": "gauge",
        "title": "Disk Usage",
        "targets": [
          {
            "expr": "system_disk_usage_percent{mount_point=\"/data\"}",
            "refId": "A"
          }
        ],
        "format": "percent",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 70 },
            { "color": "red", "value": 90 }
          ]
        }
      }
    ],
    "refresh": "10s",
    "schemaVersion": 16,
    "version": 1
  }
}
